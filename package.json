{
	"name": "haikooz",
	"version": "2.0.0",
	"description": "11ty blog engine for Haikooz blog",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/GoOz/Haikooz.git"
	},
	"author": {
		"name": "GoOz",
		"email": "gooz@foojin.com",
		"url": "https://www.foojin.com/"
	},
	"license": "unlicense",
	"engines": {
		"node": ">=18"
	},
	"bugs": {
		"url": "https://github.com/GoOz/Haikooz/issues"
	},
	"homepage": "https://github.com/GoOz/Haikooz#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^6.0.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^2.0.3",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.7",
		"eleventy-plugin-code-demo": "^2.0.3",
		"eleventy-plugin-embed-everything": "^1.19.0",
		"eleventy-plugin-reader-bar": "^0.2.1",
		"eleventy-plugin-svg-sprite": "^2.4.2",
		"eleventy-plugin-time-to-read": "^1.3.0",
		"eleventy-plugin-unfurl": "^1.0.0",
		"eleventy-plugin-webmentions": "^2.0.0",
		"luxon": "^3.5.0",
		"markdown-it-github-alerts": "^1.0.0",
		"prism-themes": "^1.9.0",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.2"
	}
}
